{
    "name": "URL Shortener: twos.cc",
    "version": "0.0.3",
    "manifest_version": 2,
    "description": "URL shortener",
    "icons": {
        "16": "icons/icon16.png",
        "19": "icons/icon19.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": "icons/icon128.png",
        "default_title": "URL shortener",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_scripts": [],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}